{
  "name": "Facebook Activity Log Deleter",
  "description": "Facebook Activity Log Deleter",
  "version": "1.0",
  "permissions": [
    "https://www.facebook.com/*"
  ],
  "background": {
    "scripts": ["main.js", "delete.js"],
